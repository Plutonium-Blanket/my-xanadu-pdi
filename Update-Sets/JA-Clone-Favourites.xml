}]]&gt;&lt;/client_script_v2&gt;&lt;comments&gt;Inserts a copy of the current record with changes to user field.&lt;/comments&gt;&lt;condition&gt;(current.getTableName() == "sys_ui_bookmark" || current.getTableName()=="sys_ui_bookmark_group") &amp;amp;&amp;amp; current.canCreate() &amp;amp;&amp;amp; (!current.instanceOf('task') || gs.getProperty('glide.ui.task.insert') == 'true') &amp;amp;&amp;amp; (!current.instanceOf('sys_db_object'))&lt;/condition&gt;&lt;form_action&gt;true&lt;/form_action&gt;&lt;form_button&gt;false&lt;/form_button&gt;&lt;form_button_v2&gt;false&lt;/form_button_v2&gt;&lt;form_context_menu&gt;true&lt;/form_context_menu&gt;&lt;form_link&gt;false&lt;/form_link&gt;&lt;form_menu_button_v2&gt;false&lt;/form_menu_button_v2&gt;&lt;form_style/&gt;&lt;format_for_configurable_workspace&gt;false&lt;/format_for_configurable_workspace&gt;&lt;hint&gt;Create a copy of the current record with any modified values&lt;/hint&gt;&lt;isolate_script&gt;false&lt;/isolate_script&gt;&lt;list_action&gt;true&lt;/list_action&gt;&lt;list_banner_button&gt;false&lt;/list_banner_button&gt;&lt;list_button&gt;false&lt;/list_button&gt;&lt;list_choice&gt;true&lt;/list_choice&gt;&lt;list_context_menu&gt;false&lt;/list_context_menu&gt;&lt;list_link&gt;false&lt;/list_link&gt;&lt;list_save_with_form_button&gt;false&lt;/list_save_with_form_button&gt;&lt;list_style/&gt;&lt;messages/&gt;&lt;name&gt;Clone to specified user&lt;/name&gt;&lt;onclick/&gt;&lt;order&gt;200&lt;/order&gt;&lt;script&gt;&lt;![CDATA[var usr = "9925958d83231210a32e58a6feaad3d4"; //insert your desired user here
var new_grp;
var new_rec;
var order;
var color;

doInsert();

if (current.getTableName() == "sys_ui_bookmark") {
    var group_name = current.group.getDisplayValue();
	order = current.group.order;
	color = current.group.color;

    if (group_name != '') {
        var bkmk_grp = new GlideRecord('sys_ui_bookmark_group');
        bkmk_grp.addEncodedQuery('user=' + usr + '^title=' + group_name);
        bkmk_grp.query();

        if (bkmk_grp.hasNext()) {
            bkmk_grp.next();
            new_grp = bkmk_grp.sys_id;
        } else {
            var create_group = new GlideRecord('sys_ui_bookmark_group');
            create_group.initialize();
            create_group.title = group_name;
            create_group.user = usr;
			create_group.order = order;
            new_grp = create_group.insert();
        }

        var bkmk = new GlideRecord('sys_ui_bookmark');
        bkmk.get(new_rec);
        bkmk.group = new_grp;
        bkmk.update();
    }
}

function doInsert() {
    current.user = usr;
    new_rec = insertRecord(current);
}]]&gt;&lt;/script&gt;&lt;show_insert&gt;false&lt;/show_insert&gt;&lt;show_multiple_update&gt;false&lt;/show_multiple_update&gt;&lt;show_query&gt;false&lt;/show_query&gt;&lt;show_update&gt;true&lt;/show_update&gt;&lt;sys_class_name&gt;sys_ui_action&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2025-02-04 10:29:22&lt;/sys_created_on&gt;&lt;sys_domain&gt;global&lt;/sys_domain&gt;&lt;sys_domain_path&gt;/&lt;/sys_domain_path&gt;&lt;sys_id&gt;d8982dc583a31210a32e58a6feaad3d1&lt;/sys_id&gt;&lt;sys_mod_count&gt;15&lt;/sys_mod_count&gt;&lt;sys_name&gt;Clone to specified user&lt;/sys_name&gt;&lt;sys_overrides/&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_ui_action_d8982dc583a31210a32e58a6feaad3d1&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2025-02-04 11:46:55&lt;/sys_updated_on&gt;&lt;table&gt;global&lt;/table&gt;&lt;ui11_compatible&gt;false&lt;/ui11_compatible&gt;&lt;ui16_compatible&gt;false&lt;/ui16_compatible&gt;&lt;/sys_ui_action&gt;&lt;sys_es_latest_script action="INSERT_OR_UPDATE"&gt;&lt;id&gt;d8982dc583a31210a32e58a6feaad3d1&lt;/id&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2025-02-04 10:30:01&lt;/sys_created_on&gt;&lt;sys_id&gt;bdb86dc583a31210a32e58a6feaad37d&lt;/sys_id&gt;&lt;sys_mod_count&gt;0&lt;/sys_mod_count&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2025-02-04 10:30:01&lt;/sys_updated_on&gt;&lt;table&gt;sys_ui_action&lt;/table&gt;&lt;use_es_latest&gt;false&lt;/use_es_latest&gt;&lt;/sys_es_latest_script&gt;&lt;/record_update&gt;</payload>
